{
  "name": "vk_dark",
  "version": "0.1.19",
  "description": "Изменяет внешний вид Вконтакте на темный",
  "main": "build.js",
  "scripts": {
    "build-backgrounds": "lessc less/vkdark-backgrounds.less css/vkdark-backgrounds.css",
    "build-borders": "lessc less/vkdark-borders.less css/vkdark-borders.css",
    "build-colors": "lessc less/vkdark-colors.less css/vkdark-colors.css",
    "build-all": "lessc less/vkdark_all.less css/vkdark_all.css",
    "prebuild": "npm run build-all && npm run build-backgrounds && npm run build-borders && npm run build-colors",
    "build": "node build.js",
    "start": "npm run prebuild && npm run build && node error_checker.js"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Dmitiry1921/VK-Dark.git"
  },
  "keywords": [
    "vk",
    "dark",
    "theme"
  ],
  "author": "Dmitriy1921",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/Dmitiry1921/VK-Dark/issues"
  },
  "homepage": "https://github.com/Dmitiry1921/VK-Dark#readme",
  "updateURL": "https://github.com/Dmitiry1921/VK-Dark/raw/master/vkdark.meta.js",
  "downloadURL": "https://github.com/Dmitiry1921/VK-Dark/raw/master/vkdark.user.js",
  "supportURL": "https://vk.com/dmitriy1921",
  "bugsLink": "https://vk.me/join/AJQ1d7U5CANH4MRXOBNPuzB4",
  "dependencies": {
    "css": "^2.2.4",
    "less": "^3.10.3"
  },
  "husky": {
    "hooks": {
      "pre-commit": "npm --no-git-tag-version version patch && npm start"
    }
  },
  "devDependencies": {
    "husky": "^4.2.5"
  }
}
